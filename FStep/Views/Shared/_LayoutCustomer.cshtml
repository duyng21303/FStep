<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - FStep</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/FStep.styles.css" asp-append-version="true" />
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/5.3.0/css/bootstrap.min.css" rel="stylesheet">

</head>
<body>
    <header>
        @await Html.PartialAsync("_Header")
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        @await Html.PartialAsync("_Footer")
    </footer>
    @* <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script> *@
    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/resource_dashmin/lib/chart/chart.min.js"></script>
    <script src="~/resource_dashmin/lib/easing/easing.min.js"></script>
    <script src="~/resource_dashmin/lib/waypoints/waypoints.min.js"></script>
    <script src="~/resource_dashmin/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/resource_dashmin/lib/tempusdominus/js/moment.min.js"></script>
    <script src="~/resource_dashmin/lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="~/resource_dashmin/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>
    <script src="~/resource_dashmin/js/main.js"></script>
    <script src="path/to/bootstrap.bundle.min.js"></script>


</body>

</html>
